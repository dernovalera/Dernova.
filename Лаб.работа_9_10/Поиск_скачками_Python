def jump_search(arr, target):  # объявляем функцию поиска скачками
    n = len(arr)  # длина массива (чтобы знать диапазон поиска)
    step = int(n ** 0.5)  # шаг - это квадратный корень из длины массива, приведенный к целому
    prev = 0  # начальный индекс для поиска, с начала массива

    while prev < n and arr[min(step, n) - 1] < target:  # пока не вышли за границы и последний элемент блока меньше target(искомый элемент)
        prev = step  # передвигаемся к следующему блоку
        step += int(n ** 0.5)  # увеличиваем шаг на корень из n, чтобы прыгнуть дальше
        if prev >= n:  # если вышли за границы массива (все блоки просмотрены)
            return -1  # элемент не найден, выходим

    for i in range(prev, min(step, n)):  # перебираем элементы внутри текущего блока
        if arr[i] == target:  # если нашли искомый элемент
            return i  # возвращаем его индекс
    return -1  # если не нашли - возвращаем -1 (элемент отсутствует)
arr = [1, 13, 5, 22, 29]  # исходный массив
target = 22  # искомое число
index = jump_search(arr, target)  # вызываем функци.
if index != -1:  # если элемент найден, выводим его
    print(f"Элемент {target} найден по индексу {index}")
else:  # иначе сообщается об отсутствии эдемента
    print(f"Элемент {target} не найден")
